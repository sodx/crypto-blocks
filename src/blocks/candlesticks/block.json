{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "crypto-blocks/candlesticks",
  "version": "0.0.1",
  "title": "Candlestick Chart",
  "category": "crypto-blocks",
  "icon": "chart-bar",
  "description": "Real time candlestick chart",
  "supports": {
    "html": false
  },
  "attributes": {
    "source": {
      "type": "string",
      "default": "okx",
      "enum": [
        "okx",
        "mexc",
        "kraken",
        "bybit",
        "kucoin"
      ]
    },
    "pair": {
      "type": "string",
      "default": "BTC/USDT",
      "enum": [
        "BTC/USDT",
        "SOL/USDT",
        "ETH/USDT",
        "TON/USDT",
        "PEPE/USDT"
      ]
    },
    "interval": {
      "type": "string",
      "default": "15m",
      "enum": [
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "4h",
        "1d"
      ]
    }
  },
  "textdomain": "crypto-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}